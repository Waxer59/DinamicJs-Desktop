{
  "name": "dinamicjs",
  "private": true,
  "version": "1.0.0",
  "main": "main.js",
  "description": "Your javascript playground",
  "license": "MIT",
  "author": "Waxer59",
  "icon": "public/android-chrome-512x512.ico",
  "scripts": {
    "start": "yarn build && yarn electron:dev",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "electron-forge start",
    "electron:build": "electron-forge make",
    "electron:package": "electron-forge package"
  },
  "dependencies": {
    "@babel/standalone": "^7.20.12",
    "@reduxjs/toolkit": "^1.9.1",
    "@sweetalert2/theme-dark": "^5.0.15",
    "canvas-confetti": "^1.6.0",
    "downloadjs": "^1.4.7",
    "js-base64": "^3.7.4",
    "loop-protect": "^2.1.6",
    "monaco-editor": "^0.34.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "react-split": "^2.0.14",
    "sweetalert2": "^11.6.16"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-wix": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.1",
    "electron": "^22.0.0",
    "electron-package": "^0.1.0",
    "electron-wix-msi": "^5.0.0",
    "vite": "^4.0.0"
  },
  "build": {
    "appId": "Dinamicjs",
    "productName": "Dinamicjs",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/images/android-chrome-512x512.ico"
    },
    "nsis": {
      "installerIcon": "src/images/android-chrome-512x512.ico",
      "uninstallerIcon": "src/images/android-chrome-512x512.ico",
      "uninstallDisplayName": "Dinamicjs",
      "oneClick": true,
      "installerHeaderIcon": "src/images/android-chrome-512x512.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Dinamicjs"
    }
  }
}
