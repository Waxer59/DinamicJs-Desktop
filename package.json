{
  "name": "dinamicjs",
  "private": true,
  "version": "2.7.6",
  "main": "main.js",
  "description": "Your javascript playground now in desktop",
  "license": "MIT",
  "author": "Waxer59",
  "icon": "dist/images/favicon.ico",
  "scripts": {
    "start": "yarn vite:build && yarn dev",
    "vite:build": "vite build",
    "dev": "electron-forge start",
    "build": "yarn build && electron-forge make",
    "package": "electron-forge package"
  },
  "dependencies": {
    "@babel/standalone": "^7.20.12",
    "@reduxjs/toolkit": "^1.9.1",
    "@sweetalert2/theme-dark": "^5.0.15",
    "canvas-confetti": "^1.6.0",
    "downloadjs": "^1.4.7",
    "electron-squirrel-startup": "^1.0.0",
    "js-base64": "^3.7.4",
    "loop-protect": "^2.1.6",
    "monaco-editor": "^0.34.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-redux": "^8.0.5",
    "react-split": "^2.0.14",
    "sweetalert2": "^11.6.16"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.1",
    "electron": "^22.0.0",
    "electron-package": "^0.1.0",
    "electron-wix-msi": "^5.0.0",
    "vite": "^4.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "dist/images/favicon.ico"
      },
      "rebuildConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "DinamicJs",
            "icon": "dist/images/favicon.ico",
            "setupIcon": "dist/images/favicon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": "dist/images/android-chrome-512x512.png"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "icon": "dist/images/android-chrome-512x512.png"
          }
        }
      ]
    }
  }
}
